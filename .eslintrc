{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-console": [
            0
        ],
        "no-unused-vars": [
            1
        ],
        "no-empty": [
            1
        ],
        "no-extra-semi": [
            1
        ],
        "indent": [
            "error",
            4,
            { "SwitchCase": 1 }
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "warn",
            "single",
             { "allowTemplateLiterals": true }
        ],
        "semi": [
            "warn",
            "always"
        ]
    }
}
